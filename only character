#include<iostream>

using namespace std;

int main()
{
    string s;
    
    getline(cin,s);
    int n=s.length();
    string j="";
    
    for(int i=0;i<n;i++)
    {
        if((s[i]>='a' && s[i]<='z') || (s[i]>='A' && s[i]<='Z' ))
        {
           j=j+s[i];
            
        }
       
    }
     cout<<j;
}
